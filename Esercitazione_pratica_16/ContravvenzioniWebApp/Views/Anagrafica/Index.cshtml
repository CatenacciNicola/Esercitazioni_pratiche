@model IEnumerable<ContravvenzioniWebApp.Models.Anagrafica>

<h1>Anagrafica</h1>
<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Cognome</th>
            <th>Nome</th>
            <th>Indirizzo</th>
            <th>Città</th>
            <th>CAP</th>
            <th>Codice Fiscale</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.IdAnagrafica</td>
                <td>@item.Cognome</td>
                <td>@item.Nome</td>
                <td>@item.Indirizzo</td>
                <td>@item.Citta</td>
                <td>@item.Cap</td>
                <td>@item.CodFiscale</td>
            </tr>
        }
    </tbody>
</table>
<a href="/Anagrafica/Create" class="btn btn-primary">Aggiungi Nuovo</a>
