@model ContravvenzioniWebApp.Models.Anagrafica

<h1>Crea Nuova Anagrafica</h1>

<form asp-action="Create" method="post">
    <div class="form-group">
        <label asp-for="Cognome"></label>
        <input asp-for="Cognome" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Nome"></label>
        <input asp-for="Nome" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Indirizzo"></label>
        <input asp-for="Indirizzo" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Citta"></label>
        <input asp-for="Citta" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Cap"></label>
        <input asp-for="Cap" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="CodFiscale"></label>
        <input asp-for="CodFiscale" class="form-control" />
    </div>
    <button type="submit" class="btn btn-primary">Salva</button>
</form>
